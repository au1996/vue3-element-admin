{
  "name": "vue3-element-admin",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "analyzer": "vite-bundle-visualizer",
    "prettier": "prettier \"{src,mock}/**/*.{vue,js}\" --write",
    "lint:js": "eslint \"{src,mock}/**/*.{vue,js}\" ",
    "lint:js-fix": "eslint \"{src,mock}/**/*.{vue,js}\" --fix",
    "lint:css": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:css-fix": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "husky": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run prettier",
      "npm run lint:js-fix",
      "npm run lint:css-fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@vueuse/core": "^12.5.0",
    "axios": "0.27.2",
    "crypto-js": "4.2.0",
    "echarts": "5.4.1",
    "element-plus": "2.3.4",
    "js-cookie": "2.2.1",
    "mockjs": "1.1.0",
    "nprogress": "0.2.0",
    "qs": "^6.14.0",
    "screenfull": "5.1.0",
    "vue": "3.4.8",
    "vue-router": "4.1.6",
    "vuex": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@vitejs/plugin-vue": "4.5.1",
    "autoprefixer": "9.8.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.9.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.8.3",
    "rollup-plugin-esbuild": "6.1.0",
    "sass": "1.32.8",
    "stylelint": "13.11.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "vite": "5.4.2",
    "vite-bundle-visualizer": "0.10.0",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-mock": "2.3.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.8"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
